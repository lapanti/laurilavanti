{
  "name": "lauri-lavanti",
  "version": "1.0.0",
  "private": true,
  "description": "Lauri Lavanti",
  "author": "Lauri Lavanti",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "22",
    "npm": "11"
  },
  "scripts": {
    "lint": "eslint src tests",
    "lint:fix": "npm run lint -- --fix",
    "typecheck:src": "tsc --noEmit",
    "typecheck:tests": "tsc --noEmit -p tests/tsconfig.json",
    "typecheck": "npm run typecheck:src && npm run typecheck:tests",
    "predevelop": "npm run clean",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --config tests/jest.config.js",
    "test:ci": "jest --config tests/jest.config.js --coverage",
    "test:e2e-dev": "start-server-and-test develop http://localhost:8000 test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:setup": "playwright install --with-deps",
    "test:e2e:update": "npm run test:e2e --  --update-snapshots",
    "test:e2e:update-ci": "start-server-and-test develop http://localhost:8000 test:e2e:update",
    "knip": "knip",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource/ibm-plex-mono": "5.2.7",
    "@fontsource/ibm-plex-sans": "5.2.8",
    "babel-plugin-styled-components": "2.1.4",
    "date-fns": "4.1.0",
    "gatsby": "5.15.0",
    "gatsby-plugin-image": "3.15.0",
    "gatsby-plugin-manifest": "5.15.0",
    "gatsby-plugin-preconnect": "1.4.0",
    "gatsby-plugin-react-helmet": "6.15.0",
    "gatsby-plugin-sharp": "5.15.0",
    "gatsby-plugin-sitemap": "6.15.0",
    "gatsby-plugin-styled-components": "6.15.0",
    "gatsby-source-contentful": "8.16.0",
    "gatsby-transformer-sharp": "5.15.0",
    "hamburger-react": "2.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "react-icons": "5.5.0",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@contentful/rich-text-types": "17.2.4",
    "@eslint/js": "9.38.0",
    "@playwright/test": "1.56.1",
    "@stylistic/eslint-plugin": "5.5.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "22.18.12",
    "@types/react-helmet": "6.1.11",
    "babel-jest": "30.2.0",
    "babel-preset-gatsby": "3.15.0",
    "confusing-browser-globals": "1.0.11",
    "dotenv": "16.6.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-testing-library": "7.13.3",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-styled-components": "7.2.0",
    "knip": "5.66.3",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "start-server-and-test": "2.1.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  }
}
