{
  "name": "lauri-lavanti",
  "version": "1.0.0",
  "private": true,
  "description": "Lauri Lavanti",
  "author": "Lauri Lavanti",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "typecheck:src": "tsc --noEmit",
    "typecheck:tests": "tsc --noEmit -p tests/tsconfig.json",
    "typecheck": "npm run typecheck:src && npm run typecheck:tests",
    "predevelop": "npm run clean",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --config tests/jest.config.js",
    "test:e2e-dev": "start-server-and-test develop http://localhost:8000 test:e2e",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@fontsource/ubuntu": "4.5.11",
    "autoprefixer": "10.4.14",
    "gatsby": "4.25.5",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-image": "2.25.0",
    "gatsby-plugin-manifest": "4.25.0",
    "gatsby-plugin-meta-redirect": "1.1.1",
    "gatsby-plugin-postcss": "5.25.0",
    "gatsby-plugin-react-helmet": "5.25.0",
    "gatsby-plugin-sharp": "4.25.0",
    "gatsby-plugin-sitemap": "5.25.0",
    "gatsby-source-contentful": "7.23.1",
    "gatsby-transformer-sharp": "4.25.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "reading-time": "1.5.0",
    "tailwindcss": "3.2.7"
  },
  "devDependencies": {
    "@contentful/rich-text-types": "16.0.3",
    "@emotion/jest": "11.10.5",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@playwright/test": "1.31.2",
    "@savvywombat/tailwindcss-grid-areas": "2.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.4.0",
    "@types/node": "18.0.0",
    "@types/react-helmet": "6.1.6",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "babel-jest": "29.5.0",
    "babel-preset-gatsby": "2.25.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "gatsby-plugin-emotion": "7.25.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "prettier": "2.8.4",
    "start-server-and-test": "1.15.5",
    "twin.macro": "3.1.0",
    "typescript": "4.9.5"
  }
}
