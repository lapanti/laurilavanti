{
  "name": "lauri-lavanti",
  "version": "1.0.0",
  "private": true,
  "description": "Lauri Lavanti",
  "author": "Lauri Lavanti",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "22",
    "npm": "11"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "typecheck:src": "tsc --noEmit",
    "typecheck:tests": "tsc --noEmit -p tests/tsconfig.json",
    "typecheck": "npm run typecheck:src && npm run typecheck:tests",
    "predevelop": "npm run clean",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --config tests/jest.config.js",
    "test:ci": "jest --config tests/jest.config.js --coverage",
    "test:e2e-dev": "start-server-and-test develop http://localhost:8000 test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:setup": "playwright install --with-deps",
    "test:e2e:update": "npm run test:e2e --  --update-snapshots",
    "test:e2e:update-ci": "start-server-and-test develop http://localhost:8000 test:e2e:update",
    "knip": "knip"
  },
  "dependencies": {
    "@fontsource/ibm-plex-mono": "5.1.0",
    "@fontsource/ibm-plex-sans": "5.1.0",
    "babel-plugin-styled-components": "2.1.4",
    "date-fns": "4.1.0",
    "gatsby": "5.14.1",
    "gatsby-plugin-image": "3.14.0",
    "gatsby-plugin-manifest": "5.14.0",
    "gatsby-plugin-preconnect": "1.4.0",
    "gatsby-plugin-react-helmet": "6.14.0",
    "gatsby-plugin-sharp": "5.14.0",
    "gatsby-plugin-sitemap": "6.14.0",
    "gatsby-plugin-styled-components": "6.14.0",
    "gatsby-source-contentful": "8.15.0",
    "gatsby-transformer-sharp": "5.14.0",
    "hamburger-react": "2.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "react-icons": "5.4.0",
    "styled-components": "6.1.13"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.1",
    "@contentful/rich-text-types": "17.0.0",
    "@eslint/js": "9.17.0",
    "@playwright/test": "1.49.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/dotenv": "8.2.3",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.2",
    "@types/react-helmet": "6.1.11",
    "babel-jest": "29.7.0",
    "babel-preset-gatsby": "3.14.0",
    "dotenv": "16.4.7",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.10.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-testing-library": "7.1.1",
    "globals": "15.14.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-styled-components": "7.2.0",
    "knip": "5.41.1",
    "prettier": "3.4.2",
    "start-server-and-test": "2.0.9",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.2"
  }
}
