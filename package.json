{
    "name": "lauri-lavanti",
    "version": "1.0.0",
    "private": true,
    "description": "Lauri Lavanti",
    "author": "Lauri Lavanti",
    "keywords": [
        "gatsby"
    ],
    "engines": {
        "node": "16",
        "npm": "8"
    },
    "scripts": {
        "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
        "lint:fix": "npm run lint -- --fix",
        "typecheck": "tsc --noEmit",
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "jest --config tests/jest.config.js",
        "cypress:open": "cypress open",
        "cy:open": "start-server-and-test develop http://localhost:8000 cypress:open",
        "cy:run:mobile": "cypress run --config-file cypress-mobile.json",
        "cy:run:desktop": "cypress run",
        "cy:run": "npm run cy:run:mobile && npm run cy:run:desktop",
        "test:e2e": "start-server-and-test develop http://localhost:8000 cy:run"
    },
    "dependencies": {
        "@fontsource/ubuntu": "4.5.9",
        "@mdx-js/mdx": "1.6.22",
        "@mdx-js/react": "1.6.22",
        "autoprefixer": "10.4.7",
        "gatsby": "4.15.1",
        "gatsby-plugin-gatsby-cloud": "4.15.0",
        "gatsby-plugin-image": "2.15.0",
        "gatsby-plugin-manifest": "4.15.0",
        "gatsby-plugin-mdx": "3.15.0",
        "gatsby-plugin-meta-redirect": "1.1.1",
        "gatsby-plugin-postcss": "5.15.0",
        "gatsby-plugin-react-helmet": "5.15.0",
        "gatsby-plugin-sharp": "4.15.0",
        "gatsby-plugin-sitemap": "5.15.0",
        "gatsby-remark-autolink-headers": "5.15.0",
        "gatsby-source-filesystem": "4.15.0",
        "gatsby-transformer-sharp": "4.15.0",
        "postcss": "8.4.14",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "react-helmet": "6.1.0",
        "reading-time": "1.5.0",
        "tailwindcss": "3.0.24"
    },
    "devDependencies": {
        "@emotion/react": "11.9.0",
        "@emotion/styled": "11.8.1",
        "@savvywombat/tailwindcss-grid-areas": "2.0.1",
        "@testing-library/cypress": "8.0.2",
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "13.2.0",
        "@types/jest": "27.5.1",
        "@types/react-helmet": "6.1.5",
        "@typescript-eslint/eslint-plugin": "5.26.0",
        "@typescript-eslint/parser": "5.26.0",
        "babel-jest": "28.1.0",
        "babel-preset-gatsby": "2.15.0",
        "cypress": "9.7.0",
        "eslint": "8.16.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-cypress": "2.12.1",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jest": "26.2.2",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "gatsby-plugin-emotion": "7.15.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "28.1.0",
        "jest-environment-jsdom": "28.1.0",
        "prettier": "2.6.2",
        "start-server-and-test": "1.14.0",
        "twin.macro": "2.8.2",
        "typescript": "4.7.3"
    }
}
