{
  "name": "lauri-lavanti",
  "version": "1.0.0",
  "private": true,
  "description": "Lauri Lavanti",
  "author": "Lauri Lavanti",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "typecheck:src": "tsc --noEmit",
    "typecheck:tests": "tsc --noEmit -p tests/tsconfig.json",
    "typecheck": "npm run typecheck:src && npm run typecheck:tests",
    "predevelop": "npm run clean",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --config tests/jest.config.js",
    "test:ci": "jest --config tests/jest.config.js --ci --json --coverage --testLocationInResults --outputFile=report.json",
    "test:e2e-dev": "start-server-and-test develop http://localhost:8000 test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:setup": "playwright install --with-deps"
  },
  "dependencies": {
    "@fontsource/ubuntu": "5.0.13",
    "autoprefixer": "10.4.19",
    "babel-plugin-styled-components": "2.1.4",
    "gatsby": "5.13.7",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-image": "3.13.1",
    "gatsby-plugin-manifest": "5.13.1",
    "gatsby-plugin-meta-redirect": "1.1.1",
    "gatsby-plugin-react-helmet": "6.13.1",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-plugin-sitemap": "6.13.1",
    "gatsby-plugin-styled-components": "6.13.1",
    "gatsby-source-contentful": "8.13.2",
    "gatsby-transformer-sharp": "5.13.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "react-icons": "5.2.1",
    "reading-time": "1.5.0",
    "styled-components": "6.1.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.9.1",
    "@contentful/rich-text-types": "16.6.1",
    "@playwright/test": "1.45.1",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.10",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "babel-jest": "29.7.0",
    "babel-preset-gatsby": "3.13.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-styled-components": "7.2.0",
    "prettier": "3.3.2",
    "start-server-and-test": "2.0.4",
    "typescript": "5.5.3"
  }
}
