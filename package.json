{
    "name": "lauri-lavanti",
    "version": "1.0.0",
    "private": true,
    "description": "Lauri Lavanti",
    "author": "Lauri Lavanti",
    "keywords": [
        "gatsby"
    ],
    "engines": {
        "node": "16",
        "npm": "8"
    },
    "scripts": {
        "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
        "lint:fix": "npm run lint -- --fix",
        "typecheck:src": "tsc --noEmit",
        "typecheck:tests": "tsc --noEmit -p tests/tsconfig.json",
        "typecheck:cy": "tsc --noEmit -p tests/cypress/tsconfig.json",
        "typecheck": "npm run typecheck:src && npm run typecheck:tests && npm run typecheck:cy",
        "predevelop": "npm run clean",
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "jest --config tests/jest.config.js",
        "cypress:open": "cypress open",
        "cy:open-dev": "start-server-and-test develop http://localhost:8000 cypress:open",
        "cy:open": "npm run cypress:open",
        "cy:run:mobile": "CY_MOBILE=true cypress run",
        "cy:run:desktop": "cypress run",
        "cy:run": "npm run cy:run:mobile && npm run cy:run:desktop",
        "test:e2e-dev": "start-server-and-test develop http://localhost:8000 cy:run",
        "test:e2e": "npm run cy:run"
    },
    "dependencies": {
        "@fontsource/ubuntu": "4.5.11",
        "autoprefixer": "10.4.12",
        "gatsby": "4.24.4",
        "gatsby-plugin-google-fonts": "1.0.1",
        "gatsby-plugin-image": "2.24.0",
        "gatsby-plugin-manifest": "4.24.0",
        "gatsby-plugin-meta-redirect": "1.1.1",
        "gatsby-plugin-postcss": "5.24.0",
        "gatsby-plugin-react-helmet": "5.24.0",
        "gatsby-plugin-sharp": "4.24.0",
        "gatsby-plugin-sitemap": "5.24.0",
        "gatsby-source-contentful": "7.22.0",
        "gatsby-transformer-sharp": "4.24.0",
        "postcss": "8.4.18",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-helmet": "6.1.0",
        "reading-time": "1.5.0",
        "tailwindcss": "3.2.1"
    },
    "devDependencies": {
        "@contentful/rich-text-types": "15.13.2",
        "@emotion/jest": "11.10.0",
        "@emotion/react": "11.10.4",
        "@emotion/styled": "11.10.4",
        "@savvywombat/tailwindcss-grid-areas": "2.0.1",
        "@testing-library/cypress": "8.0.3",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/jest": "28.1.8",
        "@types/node": "18.0.0",
        "@types/react-helmet": "6.1.5",
        "@typescript-eslint/eslint-plugin": "5.40.1",
        "@typescript-eslint/parser": "5.40.1",
        "babel-jest": "28.1.3",
        "babel-preset-gatsby": "2.24.0",
        "cypress": "10.10.0",
        "eslint": "8.26.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-cypress": "2.12.1",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jest": "26.9.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "gatsby-plugin-emotion": "7.24.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "28.1.3",
        "jest-environment-jsdom": "28.1.3",
        "prettier": "2.7.1",
        "start-server-and-test": "1.14.0",
        "twin.macro": "2.8.2",
        "typescript": "4.8.4"
    }
}
