{
  "name": "lauri-lavanti",
  "version": "1.0.0",
  "private": true,
  "description": "Lauri Lavanti",
  "author": "Lauri Lavanti",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "typecheck:src": "tsc --noEmit",
    "typecheck:tests": "tsc --noEmit -p tests/tsconfig.json",
    "typecheck": "npm run typecheck:src && npm run typecheck:tests",
    "predevelop": "npm run clean",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --config tests/jest.config.js",
    "test:e2e-dev": "start-server-and-test develop http://localhost:8000 test:e2e",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@fontsource/ubuntu": "5.0.8",
    "autoprefixer": "10.4.16",
    "gatsby": "5.13.1",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-image": "3.13.0",
    "gatsby-plugin-manifest": "5.13.0",
    "gatsby-plugin-meta-redirect": "1.1.1",
    "gatsby-plugin-postcss": "6.13.0",
    "gatsby-plugin-react-helmet": "6.13.0",
    "gatsby-plugin-sharp": "5.13.0",
    "gatsby-plugin-sitemap": "6.13.0",
    "gatsby-source-contentful": "8.13.0",
    "gatsby-transformer-sharp": "5.13.0",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "reading-time": "1.5.0",
    "tailwindcss": "3.3.5"
  },
  "devDependencies": {
    "@contentful/rich-text-types": "16.3.0",
    "@emotion/jest": "11.11.0",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@playwright/test": "1.39.0",
    "@savvywombat/tailwindcss-grid-areas": "3.1.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.0",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "babel-jest": "29.7.0",
    "babel-preset-gatsby": "3.13.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "gatsby-plugin-emotion": "8.13.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.0.3",
    "start-server-and-test": "2.0.1",
    "twin.macro": "3.4.0",
    "typescript": "5.2.2"
  }
}
