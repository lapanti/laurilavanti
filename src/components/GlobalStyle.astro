<style is:global>
    *,
    ::before,
    ::after {
        border-color: currentcolor;
        border-style: solid;
        border-width: 0px;
        box-sizing: border-box;
    }
    @font-face {
        font-display: swap;
        font-family: 'Krana Fat';
        font-style: normal;
        font-weight: 1000;
        src: url(/fonts/Krana_Fat_B.otf) format('opentype');
    }

    /* ibm-plex-mono-regular - latin_latin-ext */
    @font-face {
        font-display: swap; /* Check https://developer.mozilla.org/en-US/docs/Web/CSS/@font-face/font-display for other options. */
        font-family: 'IBM Plex Mono';
        font-style: normal;
        font-weight: 400;
        src:
            url(/fonts/ibm-plex-mono-v20-latin_latin-ext-regular.woff2) format('woff2'),
            /* Chrome 36+, Opera 23+, Firefox 39+, Safari 12+, iOS 10+ */
                url(/fonts/ibm-plex-mono-v20-latin_latin-ext-regular.ttf) format('truetype'); /* Chrome 4+, Firefox 3.5+, IE 9+, Safari 3.1+, iOS 4.2+, Android Browser 2.2+ */
    }

